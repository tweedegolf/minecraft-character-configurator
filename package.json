{
  "name": "framerate-checker",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.1.20",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-1": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": ">=12.0.0",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "exorcist": ">=0.4.0",
    "local-web-server": "^0.5.16",
    "uglify": "^0.1.5",
    "watchify": ">=3.6.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "flux": "^2.1.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "three": "^0.72.0",
    "react-three": "^0.7.3"
  },
  "scripts": {
    "start": "ws",
    "watch": "watchify lib/* js/app.js -t [babelify --presets [ es2015 react] --plugins [ transform-class-properties ] ] -o build/app.js --debug --verbose",
    "build": "browserify js/app.js -t [ babelify --presets [ es2015 react stage-1] ] --debug | exorcist build/app.js.map | uglifyjs -c > build/app2.js"
  }
}
